/* 学校概括 */
.interval {
  height: 16rpx;
  background-color: #f2f2f2;
}
.particulars .generalize{
  width: 690rpx;
  margin: 0 auto;
}
.particulars .generalize .simple{
  margin-top:37rpx; 
}
.particulars .simple .universitySimple{
 display: flex;
 justify-content: flex-start;
}
.particulars .universitySimple image{
  width: 114rpx;
  height: 114rpx;
}
.particulars .universitySimple .msgBox{
  display: flex;
  flex-direction:column;
  justify-content: center;
   padding-left:20rpx; 
}
.particulars .universitySimple .msgBox .name{
  font-size: 32rpx;
  font-weight: bold;
  color: #323232;
 
}
.particulars .levelMsg{
  font-size: 24rpx;
  color: #a5a5a5;
  padding-top:20rpx; 
}
.particulars .simple .particularBox{
  display: flex;
  flex-wrap: wrap;
 margin-top: 17rpx;
}
.particulars .simple .particularBox view{
  font-size: 24rpx;
  margin-top:20rpx; 
  color: #323232;
}
.particulars .simple .particularBox view text:nth-child(1){
 color: #a5a5a5;
}
.particulars .simple .particularBox view:nth-child(3n+1){
  width: 220rpx;
}
.particulars .simple .particularBox view:nth-child(3n+2){
  width: 175rpx;
}
.particulars .simple .particularBox view:nth-child(7){
  width: 300rpx;
}
.particulars .filtrate{
  display: flex;
  flex-wrap: wrap;
  margin-top: 24rpx;
} 
.particulars .filtrate view{
  width: 208rpx;
	height: 70rpx;
	background-color: #f2f2f2;
	border-radius: 10rpx;
  line-height: 70rpx;
  text-align: center;
  margin-right:32rpx; 
  margin-top: 23rpx;
} 
.particulars .filtrate view:nth-child(3n){
  margin-right:0px; 
}
.particulars .filtrate .act{
  background-color: #419aff;
  color: #fff;
}
.particulars .content{
  padding-left:30rpx; 
}
.particulars .content .box{
  font-size: 28rpx;
  font-weight: 900;
  font-family: PingFang-SC-Bold;
  padding-bottom: 25rpx;
  padding-right: 30rpx;
}

.particulars .content .box .text{
  margin-top: 25rpx;
}
.particulars .content .box rich-text {
  font-size: 28rpx;
  font-weight: normal;
  color:#323232;
  line-height: 50rpx;
}
.particulars .content .box:first-child{
  margin-top:70rpx; 
}
.particulars .content .box:last-child{
  margin-bottom:0rpx; 
}
.particulars .box .textBox view{
  font-size: 28rpx;
  color:#323232;
  font-weight: normal;
  line-height:50rpx;
} 
.particulars .content .box .classify{
  padding-bottom:30rpx; 
  border-bottom: #f2f2f2 1rpx solid;
}
.particulars .total{
 margin-top:25rpx;
}
.particulars .lab{
  margin-top: 60rpx;
}
.particulars .lab view:first-child{
  padding-bottom: 60rpx;
}


/* 专业分数线 */
.major .interval {
  height: 16rpx;
  background-color: #f2f2f2;
}
.major .product{
  margin-top: 60rpx;
}
.major .content {
  padding: 0 30rpx ;
}

.major .setting {
  height: 80rpx;
  font-size: 24rpx;
  color: #419aff;
  line-height: 80rpx;
}

.major .select {
  display: flex;
  justify-content: space-between;
  width: 690rpx;
}

.major .select>view {
  font-family: PingFang-SC-Bold;
  width: 182rpx;
  height: 67rpx;
  border: solid 1rpx #eee;
  border-radius: 15rpx;
  line-height: 67rpx;
  text-align: left;
  padding-left: 25rpx;
  color: #323232;
  position: relative;
  font-weight: bold;
}

.major .select>view::after {
  content: "";
  width: 0;
  height: 0;
  border-width: 10rpx 9rpx 0;
  border-style: solid;
  border-color: #a5a5a5 transparent transparent;
  position: absolute;
  top: 29rpx;
  right: 23rpx;
}

.major .majorNmae {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 32rpx 0 0;
  height: 120rpx;
  border-bottom: 1rpx solid #f2f2f2;
  color: #323232;
  font-size: 28rpx;
}
.major .majorNmae text:nth-child(2){
  color: #a5a5a5;
  font-size: 24rpx;
}

/* 历史分数线 */

