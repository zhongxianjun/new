.records .yearNav{
  display: flex;
  position: relative;
}
.records .yearNav .xiantiao{
  height: 4rpx;
  width: 20%;
  background: #419aff;
  position: absolute;
  left: 0;
  bottom: 0;
  transition: all 0.5s;
}
.records .yearNav view{
  width: 25%;
 font-size: 28rpx;
 color: #323232;
 text-align: center;
 height: 85rpx;
 line-height: 85rpx;
 margin-top: 36rpx;
}
.records .Megbox{
  display: flex;
  flex-wrap: wrap;
  width: 690rpx;
  margin: 0 auto;
}
.records .Megbox view{
  margin-top: 42rpx;
}
.records .Megbox view text{
  color: #419aff;
}
.records .Megbox view:nth-child(3n+1){
  width: 265rpx;
}
.records .Megbox view:nth-child(3n+2){
  width: 259rpx;
}
.records .admit{
  width: 360rpx;
	height: 88rpx;
	background-image: linear-gradient(90deg, 
		#508eff 0%, 
		#39bafe 100%), 
	linear-gradient(
		#0d69ba, 
		#0d69ba);
	background-blend-mode: normal, 
		normal;
    margin: 0 auto;
    color: #ffffff;
    font-size: 32rpx;
    line-height: 88rpx;
    text-align: center;
    border-radius: 44rpx;
    margin-top: 50rpx;
}
.records .explain{
  display: flex;
  justify-content: center;
  margin-top: 58rpx;
}
.records .explain view{
  width: 120rpx;
  font-size: 20rpx;
  color: #a5a5a5;
}
.records .explain .topScore text{
  display: inline-block;
  width: 16rpx;
	height: 16rpx;
	background-color: #5688c1;
  border-radius: 50%;
  margin-right: 15rpx;
 
}
.records .explain .control text{
  display: inline-block;
  width: 16rpx;
	height: 16rpx;
background-color: #af9fca;
  border-radius: 50%;
   margin-right: 15rpx;
   
}
.records .explain .average text{
  display: inline-block;
  width: 16rpx;
	height: 16rpx;
background-color: #ca7aa3;
  border-radius: 50%;
   margin-right: 15rpx;
 
}
.records .qxt{
  width: 690rpx;
  margin: 0 auto;
  margin-top: 70rpx;
 
}
.records .qxt image{
  height: 359rpx;
  width: 690rpx;
  display: block;
}

